<Window x:Class="Project.Forms.SrokDogovora"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Project.Forms"
        mc:Ignorable="d"
        Title="SrokDogovora" Height="128.438" Width="200.938">
    <Grid>
        <StackPanel>
            <ComboBox x:Name="tb_srok" Margin="5" materialDesign:HintAssist.Hint="Срок договора" Style="{DynamicResource MaterialDesignFloatingHintComboBox}" SelectionChanged="tb_srok_SelectionChanged"/>
            <Button x:Name="bt_srok" Content="Продлить" Margin="5" FontSize="18" Click="cl_toSave" IsEnabled="False">
            </Button>
        </StackPanel>
    </Grid>
</Window>
